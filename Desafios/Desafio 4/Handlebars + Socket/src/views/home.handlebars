{{!-- Lista de productos con Handlebars --}}
<div class ="infoProd">
        <h1>Lista de Productos con Handlebars</h1>
            <div class="detalleProd">
            <table>
            <thead>
                <tr>
                    <th>Título</th>
                    <th>Thumbnails</th>
                    <th>Descripción</th>
                    <th>Código</th>
                    <th>Precio</th>
                    <th>Estado</th>
                    <th>Stock</th>
                    <th>Categoría</th>
                    <th>Thumbnails</th>
                </tr>
            </thead>
            <tbody>
                {{#each products}}
                    <tr>
                        <td>{{this.title}}</td>
                        <td>{{this.thumbnails}}</td>
                        <td>{{this.description}}</td>
                        <td>{{this.code}}</td>
                        <td>{{this.price}}</td>
                        <td>{{this.status}}</td>
                        <td>{{this.stock}}</td>
                        <td>{{this.category}}</td>
                        <td>{{this.thumbnails}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
            </div>
        </div>
